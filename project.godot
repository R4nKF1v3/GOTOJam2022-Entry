; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "AudioHandler",
"language": "GDScript",
"path": "res://utils/AudioHandler.gd"
}, {
"base": "EditorVCSInterface",
"class": "GitAPI",
"language": "NativeScript",
"path": "res://addons/godot-git-plugin/git_api.gdns"
}, {
"base": "Node2D",
"class": "Player",
"language": "GDScript",
"path": "res://game/player/Player.gd"
}, {
"base": "Node2D",
"class": "Scenario",
"language": "GDScript",
"path": "res://game/scenarios/Scenario.gd"
}, {
"base": "Node",
"class": "Screen",
"language": "GDScript",
"path": "res://screens/DefaultScreen.gd"
} ]
_global_script_class_icons={
"AudioHandler": "",
"GitAPI": "",
"Player": "",
"Scenario": "",
"Screen": ""
}

[application]

config/name="Demens Dementatus"
run/main_scene="res://Main.tscn"
config/icon="res://icon.png"

[autoload]

GameState="*res://globals/GameState.tscn"
AudioManager="*res://globals/AudioManager.tscn"

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=960
window/size/test_height=540
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor]

version_control_autoload_on_startup=true
version_control_plugin_name="GitAPI"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[mono]

project/assembly_name="GOTOGameJam"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
